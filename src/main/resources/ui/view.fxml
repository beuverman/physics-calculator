<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.SpinnerValueFactory.IntegerSpinnerValueFactory?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.VBox?>


<VBox fx:id="scene" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.CalculatorController">
   <children>
      <ToolBar prefHeight="30.0">
        <items>
            <Button mnemonicParsing="false" onMouseClicked="#saveDialog" text="Save" />
            <Button mnemonicParsing="false" onMouseClicked="#loadDialog" text="Load" />
            <Button mnemonicParsing="false" onMouseClicked="#clear" text="Clear" />
            <Spinner fx:id="sigFigSpinner" editable="true" prefHeight="25.0" prefWidth="54.0">
              <valueFactory>
                <SpinnerValueFactory.IntegerSpinnerValueFactory initialValue="6" max="10" min="1" />
              </valueFactory>
            </Spinner>
        </items>
      </ToolBar>
       <ScrollPane fx:id="scrollPane"></ScrollPane>
   </children>
</VBox>